foreign {

gt_val read_int(void) {
  int n = 0;
  int c;
  do {
    c = getchar();
  } while (c == '\n' || c == ' ');
  while ('0' <= c && c <= '9') {
    n = 10*n + (c - '0');
    c = getchar();
  }
  return (gt_val)(long)n;
}

void write_int(gt_val c) { printf("%d\n", (int)(long)c); }

gt_val add(gt_val x, gt_val y) { return (gt_val)((long)x + (long)y); }

}

loop
  x <~ read_int;
  y <~ read_int;
  xy <~ add x y;
  do write_int xy.

